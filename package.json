{
  "name": "duplicalis",
  "version": "1.1.0",
  "description": "CLI to detect duplicate or near-duplicate React components (code + styles).",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "duplicalis": "bin/duplicalis.js"
  },
  "scripts": {
    "start": "node ./bin/duplicalis.js --help",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "test": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "react",
    "duplicate-detection",
    "cli",
    "embedding"
  ],
  "author": "Pavel Frankov <pavel@frankov.ru>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfrankov/duplicalis.git"
  },
  "bugs": {
    "url": "https://github.com/pfrankov/duplicalis/issues"
  },
  "homepage": "https://github.com/pfrankov/duplicalis#readme",
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/parser": "^7.24.6",
    "@babel/traverse": "^7.24.6",
    "@babel/types": "^7.24.6",
    "@xenova/transformers": "^2.17.2",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "micromatch": "^4.0.7",
    "msgpackr": "^1.11.5",
    "onnxruntime-node": "^1.19.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write"
    ]
  }
}
